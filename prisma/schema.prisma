generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id    String  @id @default(cuid())
    email String  @unique
    name  String?
}

model Record {
    id         String   @id @default(cuid())
    office     String
    url        String
    clickTimes Int
    updatedAt  DateTime @updatedAt
}
